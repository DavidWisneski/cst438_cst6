<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>NameServlet</servlet-name>
  	<display-name>NameServlet</display-name>
  	<description></description>
  	<servlet-class>dw.NameServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>MyServlet</servlet-name>
  	<display-name>MyServlet</display-name>
  	<description></description>
  	<servlet-class>dw.MyServlet</servlet-class>
  </servlet>
   <servlet>
  	<servlet-name>Hangman</servlet-name>
  	<display-name>Hangman</display-name>
  	<description></description>
  	<servlet-class>cst.Hangman</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>NameServlet</servlet-name>
  	<url-pattern>/NameServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MyServlet</servlet-name>
  	<url-pattern>/MyServlet</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
  	<servlet-name>Hangman</servlet-name>
  	<url-pattern>/Hangman</url-pattern>
  </servlet-mapping>
</web-app>
